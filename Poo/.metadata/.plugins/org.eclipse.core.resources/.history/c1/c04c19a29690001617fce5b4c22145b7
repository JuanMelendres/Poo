		this.turno = true;
	}
	public boolean pedirCoordenada(){
		String coordenada = JOptionPane.showInputDialog(null, "Ingresa la coordenada donde quiers tirar en X");
		return turno;
		
	}
//	public boolenagano
	public static void main(String[] args) {
